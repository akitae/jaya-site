<!DOCTYPE html>
    <html lang="fr">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}jAYA Reborn{% endblock %}</title>

        {% stylesheets filter="cssrewrite"
            'css/bootstrap.min.css'
            'css/front/navbar.css'
            'css/front/footer.css'
            'css/front/global.css'
            'css/front/jaya.css'
            output='css/compiled/baseJaya.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
        {% endstylesheets %}
        {% block stylesheets %}{% endblock %}
    </head>

    <body>
        <div class="container">
            {% block navbar %}
                {% include '/jaya/navbar.html.twig' %}
            {% endblock %}

            {% block header %}{% endblock %}

            {% block body %}{% endblock %}

            {%  block footer %}
                {% include '/jaya/footer.html.twig' %}
            {% endblock %}
        </div>
    </body>

    {% javascripts
        'js/jquery.min.js'
        'js/bootstrap.min.js'
        'js/front/global.js'
        'js/choixUe.js'
        'js/sortable/Sortable.js'
        output='js/compiled/appJaya.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% block javascripts %}{% endblock %}

</html>
